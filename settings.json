    "editor.fontFamily": "'Sarasa Mono SC'",
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": false
  },
  "[scss]": {
    "editor.rulers": [80, 120, 140]
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.rulers": [80, 120, 140],
    "files.trimTrailingWhitespace": false
  },
  "[xml]": {
    "editor.rulers": [80, 120, 140],
    "files.trimTrailingWhitespace": false
