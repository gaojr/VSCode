{
    "workbench.settings.editor": "json",
    "workbench.iconTheme": "vs-seti",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.colorCustomizations": {
        "activityBar.background": "#3f3f3f",
        "editor.background": "#00000000",
        "terminal.background": "#000000",
        "terminal.foreground": "#cccccc",
        "titleBar.activeBackground": "#3f3f3f",
        "statusBar.background": "#3f3f3f"
    },
    "workbench.enableExperiments": false,
    "workbench.activityBar.visible": true,
    "workbench.statusBar.visible": true,
    "workbench.statusBar.feedback.visible": false,
    "editor.multiCursorModifier": "alt",
    "editor.fontFamily": "'PT Mono','Fira Code',Monaco,'Courier New','Microsoft YaHei',monospace",
    "editor.scrollBeyondLastLine": false,
    "editor.detectIndentation": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.parameterHints.cycle": true,
    "editor.quickSuggestionsDelay": 100,
    "editor.snippetSuggestions": "top",
    "editor.codeActionsOnSaveTimeout": 500,
    "editor.formatOnType": true,
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "files.autoGuessEncoding": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.exclude": {
        "* */.git": true,
        "* */.svn": true,
        "* */.hg": true,
        "* */CVS": true,
        "* */.DS_Store": true
    },
    "files.associations": {
        // "plaintext": "markdown",
        "*.extension": "html",
        "*.txt": "markdown"
    },
    "window.menuBarVisibility": "default",
    "window.newWindowDimensions": "inherit",
    "window.restoreWindows": "all",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\cmd.exe",
    /* 扩展相关 */
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "breadcrumbs.enabled": true,
    /* 同步 */
    "syncing.extensions.autoUpdate": true,
    "syncing.excludedSettings": [],
    "syncing.excludedExtensions": [],
    /* 路径补全 */
    "path-intellisense.absolutePathToWorkspace": true,
    "path-intellisense.autoSlashAfterDirectory": true,
    "path-intellisense.extensionOnImport": true,
    "path-intellisense.mappings": {},
    "path-intellisense.showHiddenFiles": true,
    /* 比较 */
    "partialDiff.hideCommandsOnContextMenu": false,
    "partialDiff.preComparisonTextNormalizationRules": [
        {
            "name": "Replace tabs with whitespaces",
            "match": "\t",
            "replaceWith": "    "
        },
        {
            "name": "One space after comma",
            "match": ", *([^,\n]+)",
            "replaceWith": ", $1"
        }
    ],
    /* git */
    "git.enabled": true,
    "git.path": "D:/Program Files/Git/bin/git.exe",
    "git.ignoreMissingGitWarning": true,
    "git.ignoreLegacyWarning": true,
    "gitlens.gitExplorer.files.layout": "list",
    "gitlens.resultsExplorer.files.layout": "list",
    "gitlens.historyExplorer.enabled": false,
    "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm",
    "gitlens.defaultDateStyle": "relative",
    "gitlens.defaultGravatarsStyle": "retro",
    "gitlens.blame.avatars": false,
    "gitlens.blame.ignoreWhitespace": true,
    "gitlens.blame.dateFormat": "YYYY-MM-DD",
    "gitlens.blame.format": "${id|7}", // 责问
    "gitlens.blame.heatmap.location": "left",
    "gitlens.blame.highlight.enabled": true,
    "gitlens.blame.highlight.locations": [
        // "gutter",// 行号
        "line", // 行
        "overview" // 滚动栏
    ],
    "gitlens.currentLine.enabled": true,
    "gitlens.currentLine.dateFormat": "YYYY-MM-DD",
    "gitlens.currentLine.format": "${author},${date}", // 当前行
    // ${path} = ${directory}/${file}
    // ${filePath} ≈ ${file} · ${directory}
    "gitlens.explorers.statusFileFormat": "${working}${filePath}", //gitlens file diff
    "gitlens.explorers.commitFormat": "${author}>${message}", //gitlens branches
    "gitlens.explorers.commitFileFormat": "${filePath}",
    "gitlens.explorers.stashFormat": "${author}>${message}", //gitlens stashes
    "gitlens.explorers.stashFileFormat": "${filePath}",
    "gitlens.explorers.avatars": true,
    "gitlens.statusBar.enabled": true,
    "gitlens.statusBar.alignment": "left",
    "gitlens.statusBar.command": "gitlens.diffWithWorking",
    "gitlens.statusBar.dateFormat": "YYYY-MM-DD HH:mm",
    "gitlens.statusBar.format": "${date} (${id}) ${message}", // 状态栏
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.menus": {
        "editor": {
            "compare": false,
            "history": false,
            "remote": false,
            "blame": false,
            "clipboard": true,
            "details": false
        },
        "editorGroup": {
            "compare": true,
            "history": true,
            "remote": false,
            "blame": false
        },
        "editorTab": {
            "compare": true,
            "history": true,
            "remote": false
        },
        "explorer": {
            "compare": true,
            "history": true,
            "remote": false
        }
    },
    "gitHistory.pageSize": 150,
    "gitHistory.showEditorTitleMenuBarIcons": false,
    /* svn */
    "svn.enabled": true,
    "svn.path": "D:/Program Files/TortoiseSVN/bin",
    "svn.autorefresh": false,
    "svn.default.encoding": "UTF-8",
    "svn.multipleFolders.enabled": false,
    "svn.multipleFolders.ignore": [
        "**/.git",
        "**/.hg",
        "**/vendor",
        "**/node_modules"
    ],
    /* markdown */
    "markdown.preview.breaks": true,
    "markdown.preview.fontFamily": "'PT Serif','Times New Roman','Hiragino Sans GB','Microsoft YaHei'",
    "markdown.preview.lineHeight": 1.5,
    "markdown.styles": [],
    "[markdown]": {
        // "files.trimTrailingWhitespace": true,
        // "editor.wordWrap": "on",
        "editor.quickSuggestions": false
    },
    "markdownExtended.imageOmitBackground": true,
    "markdownExtended.pdfDisplayHeaderFooter": true,
    "markdownExtended.pdfHeaderTemplate": "<div style='font-size: 9px; margin: 0 auto;'><span class='title'></span></div><div style='font-size: 9px; margin: 0 auto;'><span class='date'></span></div>",
    "markdownExtended.pdfFooterTemplate": "<div style='font-size: 9px; margin: 0 auto;'><span class='pageNumber'></span> / <span class='totalPages'></span></div>",
    "markdownExtended.pdfMarginTop": "0cm",
    "markdownExtended.pdfMarginBottom": "0cm",
    "markdownExtended.pdfMarginLeft": "0cm",
    "markdownExtended.pdfMarginRight": "0cm",
    "markdown-preview-enhanced.codeBlockTheme": "auto.css",
    "markdown-preview-enhanced.previewTheme": "one-light.css",
    "markdown-preview-enhanced.singlePreview": false,
    /* java */
    "[java]": {
        "editor.rulers": [
            80,
            120
        ],
        "editor.wordWrap": "off"
    },
    "java.autobuild.enabled": false,
    "java.home": "D:\\Java\\jdk1.8.0_181",
    "java.debug.logLevel": "info",
    "java.errors.incompleteClasspath.severity": "ignore",
    // "java.configuration.maven.userSettings": null,
    "java.configuration.updateBuildConfiguration": "interactive",
    // "java.contentProvider.preferred": null,
    "java.implementationsCodeLens.enabled": false,
    "java.import.gradle.enabled": false,
    "java.import.maven.enabled": true,
    "java.format.enabled": true,
    "java.format.comments.enabled": true,
    // "java.format.settings.profile": "Eclipse",// .settings/org.eclipse.jdt.core.prefs中
    "java.format.settings.url": "D:\\Program Files\\Microsoft VS Code\\resources\\app\\extensions\\java\\format\\Thunisoft_Develop_1.xml",
    "java.format.onType.enabled": true,
    "java.completion.importOrder": [
        "java",
        "javax",
        "org",
        "com"
    ],
    "java.jdt.ls.vmargs": "-noverify -Xmx1G -XX:+UseG1GC -XX:+UseStringDeduplication -javaagent:\"C:\\Users\\Gao\\.vscode\\extensions\\gabrielbb.vscode-lombok-0.9.6/server/lombok.jar\" -Xbootclasspath/a:\"C:\\Users\\Gao\\.vscode\\extensions\\gabrielbb.vscode-lombok-0.9.6/server/lombok.jar\"",
    /* javascript */
    "[javascript]": {
        "editor.rulers": [
            80,
            120
        ],
        "editor.wordWrap": "off"
    },
    "javascript.format.enable": false,
    "javascript.format.insertSpaceAfterCommaDelimiter": true,
    "javascript.format.insertSpaceAfterConstructor": false,
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
    "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": false,
    "javascript.format.insertSpaceAfterSemicolonInForStatements": true,
    "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": false,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": false,
    /* html */
    "[html]": {
        "editor.rulers": [
            80,
            120
        ],
        "editor.wordWrap": "off"
    },
    "html.format.enable": false,
    "html.format.endWithNewline": false,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.extraLiners": "head,body,/html",
    "html.format.contentUnformatted": "pre,code,textarea",
    "html.format.unformatted": "wbr,b,i",
    "html.format.maxPreserveNewLines": 1,
    "html.format.preserveNewLines": true,
    // "html.format.wrapAttributes": "auto",
    "html.format.wrapLineLength": 120,
    "htmlhint.enable": true,
    "htmlhint.options": {
        "tagname-lowercase": true,
        "attr-lowercase": true,
        "attr-value-double-quotes": false,
        "doctype-first": false,
        "tag-pair": true,
        "spec-char-escape": true,
        "id-unique": true,
        "src-not-empty": true,
        "attr-no-duplication": true,
        "title-require": true
    },
    /* css */
    "[css]": {
        "editor.rulers": [
            80,
            120
        ],
        "editor.wordWrap": "off"
    },
    "css.validate": true,
    "css.lint.duplicateProperties": "warning",
    "css.lint.emptyRules": "ignore",
    "css.lint.fontFaceProperties": "ignore",
    "css.lint.zeroUnits": "warning",
    /* beautify */
    "beautify.config": {
        // all
        "eol": "\n", // 默认:"\n"
        "end_with_newline": false, // 默认:false
        "indent_char": " ", // 默认:" "
        "indent_size": 4, // 默认:4
        "indent_with_tabs": false, // 默认:false
        "preserve_newlines": true, // 默认:true
        // js, html
        "brace_style": "collapse,preserve-inline", // 默认:"collapse"
        "max_preserve_newlines": 1, // 默认:10
        "wrap_line_length": 0, // 默认:0
        // js
        "break_chained_methods": false, // 默认:false
        "comma_first": false, // 默认:false
        "e4x": false, // 默认:false
        "indent_level": 0, // 默认:0
        "jslint_happy": false, // 默认:false
        "keep_array_indentation": true, // 默认:false
        "keep_function_indentation": false, // 默认:false
        "operator_position": "after-newline", // 默认:"before-newline"
        "space_after_anon_function": true, // 默认:false
        "space_before_conditional": true, // 默认:true
        "space_in_empty_paren": false, // 默认:false
        "space_in_paren": false, // 默认:false
        "unescape_strings": false, // 默认:false
        "unindent_chained_methods": false, // 默认:false
        // html
        "extra_liners": [
            // 默认
            "head",
            "body",
            "/html"
            // 自定义
        ],
        "indent_body_inner_html": true, // 默认:true
        "indent_handlebars": false, // 默认:false
        "indent_head_inner_html": true, // 默认:true
        "indent_inner_html": false, // 默认:false
        "indent_scripts": "normal", // 默认:"normal"
        "wrap_attributes": "auto", // 默认:"auto"
        "wrap_attributes_indent_size": 4, // 默认:同'indent_size'
        "unformatted": [
            // 默认
            // "a",
            "abbr",
            "area",
            "audio",
            "b",
            "bdi",
            "bdo",
            // "br",
            // "button",
            "canvas",
            "cite",
            "code",
            "data",
            "datalist",
            "del",
            "dfn",
            "em",
            "embed",
            "i",
            // "iframe",
            // "img",
            // "input",
            "ins",
            "kbd",
            "keygen",
            // "label",
            "map",
            "mark",
            "math",
            "meter",
            "noscript",
            "object",
            "output",
            "progress",
            "q",
            "ruby",
            "s",
            "samp",
            // "select",
            "small",
            // "span",
            "strong",
            "sub",
            "sup",
            "svg",
            "template",
            // "textarea",
            "time",
            "u",
            "var",
            "video",
            "wbr",
            "text",
            "acronym",
            "address",
            "big",
            // "dt",
            "ins",
            "strike",
            // "tt"
            // 自定义
        ],
        "content_unformatted": [
            // 默认
            "pre"
            // 自定义
        ],
        "void_elements": [
            // 默认
            "area",
            "base",
            "br",
            "col",
            "embed",
            "hr",
            "img",
            "input",
            "keygen",
            "link",
            "menuitem",
            "meta",
            "param",
            "source",
            "track",
            "wbr",
            "!doctype",
            "?xml",
            "?php",
            "basefont",
            "isindex"
            // 自定义
        ],
        // css
        "newline_between_rules": false, // 默认:false
        "selector_separator_newline": true, // 默认:true
        "space_around_combinator": false, // 默认:false
        // ？？
        // "eval_code": false, // 默认:
    },
    "beautify.ignore": [],
    "beautify.language": {
        "js": {
            "type": [
                "javascript"
            ],
            "filename": [
                ".jshintrc",
                ".jsbeautify"
            ]
        },
        "css": [
            "css",
            "scss"
        ],
        "html": [
            "htm",
            "html"
        ]
    },
    /* emmet */
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "emmet.triggerExpansionOnTab": true,
    /* xml */
    "[xml]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.rulers": [
            80,
            120
        ],
        "editor.wordWrap": "off"
    },
    "xmlTools.xmlFormatterImplementation": "v2",
    /* yaml */
    "[yaml]": {
        "editor.rulers": [
            80,
            120
        ],
        "editor.tabSize": 2,
        "editor.autoIndent": true
    },
    /* json */
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.rulers": [
            80,
            120
        ],
        "editor.wordWrap": "off"
    },
    /* python */
    "[python]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.rulers": [
            80,
            120
        ],
        "editor.wordWrap": "off"
    }
}