{
    "workbench.settings.editor": "ui",
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.activeIconPack": "vue",
    "workbench.colorTheme": "Night Owl",
    "workbench.colorCustomizations": {
        // 黑色
        // "activityBar.background": "#3f3f3f",
        // "editor.background": "#00000000",
        // "terminal.background": "#000000",
        // "terminal.foreground": "#cccccc",
        // "titleBar.activeBackground": "#3f3f3f",
        // "statusBar.background": "#3f3f3f"
        // 白色
        // "activityBar.background": "#ffffff00",
        // "editor.background": "#ffffff00",
        // "terminal.background": "#dddddd",
        // "terminal.foreground": "#000000",
        // "titleBar.activeBackground": "#3f3f3f",
        // "statusBar.background": "#3f3f3f"
    },
    "workbench.enableExperiments": false,
    "workbench.activityBar.visible": true,
    "workbench.statusBar.visible": true,
    "editor.multiCursorModifier": "alt",
    //'FiraCode QiHei NF','DejaVuSansMono YaHei NF',
    "editor.fontFamily": "'PT Mono','Microsoft YaHei UI','汉仪旗黑-55S',Monaco,'Courier New',monospace",
    "editor.fontLigatures": false,
    "editor.scrollBeyondLastLine": false,
    "editor.detectIndentation": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.quickSuggestions": {
        "strings": false,
        "comments": false,
        "other": true
    },
    "editor.parameterHints.cycle": true,
    "editor.quickSuggestionsDelay": 100,
    "editor.snippetSuggestions": "top",
    "editor.codeActionsOnSaveTimeout": 500,
    "editor.defaultFormatter": null,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 120,
    "editor.minimap.renderCharacters": true,
    "editor.wordWrap": "on",
    "zenMode.hideActivityBar": false,
    "files.autoGuessEncoding": false,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.exclude": {
        "* */.git": true,
        "* */.svn": true,
        "* */.hg": true,
        "* */CVS": true,
        "* */.DS_Store": true
    },
    "files.associations": {
        "*.ftl": "xml",
        "*.extension": "html",
        "*.txt": "markdown",
        "*.vue": "html"
    },
    "window.menuBarVisibility": "default",
    "window.newWindowDimensions": "inherit",
    "window.restoreWindows": "all",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.renderSideBySide": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true
    },
    "search.showLineNumbers": true,
    "search.useReplacePreview": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.shell.windows": "D:\\Program Files\\Git\\bin\\bash.exe",
    "breadcrumbs.enabled": true,
    "breadcrumbs.symbolSortOrder": "name",
    "update.mode": "manual",
    "update.enableWindowsBackgroundUpdates": false,
    /* 扩展相关 */
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    /* 路径补全 */
    // "path-intellisense.absolutePathToWorkspace": true,
    "path-intellisense.autoSlashAfterDirectory": true,
    "path-intellisense.extensionOnImport": true,
    // "path-intellisense.mappings": {},
    // "path-intellisense.showHiddenFiles": false,
    /* git */
    "git.enabled": true,
    "git.path": "D:/Program Files/Git/bin/git.exe",
    "git.autorefresh": true,
    "git.ignoreMissingGitWarning": true,
    "git.ignoreLegacyWarning": true,
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.command": "gitlens.toggleFileBlame",
    "gitlens.views.repositories.avatars": false,
    "gitlens.views.repositories.showBranchComparison": false,
    "gitlens.mode.statusBar.enabled": false,
    "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm",
    "gitlens.defaultDateShortFormat": "YY-M-D H:m",
    "gitlens.defaultDateStyle": "relative",
    "gitlens.defaultGravatarsStyle": "retro",
    "gitlens.blame.avatars": false,
    "gitlens.blame.ignoreWhitespace": true,
    "gitlens.blame.dateFormat": "YYYY-MM-DD",
    "gitlens.blame.format": "${id|7}", // 责问
    "gitlens.blame.heatmap.location": "left",
    "gitlens.blame.highlight.locations": [
        // "gutter",// 行号
        "line", // 行
        "overview" // 滚动栏
    ],
    "gitlens.currentLine.enabled":true,
    "gitlens.currentLine.dateFormat": "YYYY-MM-DD",
    "gitlens.currentLine.format": "${author},${date}",
    "gitlens.statusBar.alignment": "left",
    "gitlens.statusBar.dateFormat": "YYYY-MM-DD HH:mm",
    "gitlens.statusBar.format": "${date} (${id}) ${message}", // 状态栏
    "gitlens.hovers.avatars": false,
    "gitlens.hovers.annotations.over": "annotation",
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.views.commitFileFormat": "${filePath}",
    "gitlens.views.commitFormat": "${author}>${message}",
    "gitlens.views.stashFileFormat": "${filePath}",
    "gitlens.views.stashFormat": "${author}>${message}",
    "gitlens.views.statusFileFormat": "${working}${filePath}",
    "gitlens.views.compare.enabled": true,
    "gitlens.views.compare.location": "scm",
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.fileHistory.location": "scm",
    "gitlens.views.lineHistory.enabled": false,
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.views.repositories.enabled": true,
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.search.enabled": true,
    "gitlens.views.search.location": "scm",
    "gitlens.recentChanges.highlight.locations": [
        "gutter",
        "line",
        "overview"
    ],
    // "git-graph.autoCenterCommitDetailsView": true,
    // "git-graph.combineLocalAndRemoteBranchLabels": true,
    "git-graph.commitDetailsViewLocation": "Docked to Bottom",
    // "git-graph.customBranchGlobPatterns": [],
    // "git-graph.dateFormat":"Date & Time",
    "git-graph.showStatusBarItem": false,
    "git-graph.dateType": "Commit Date",
    "git-graph.defaultColumnVisibility": {
        "Date": true,
        "Author": true,
        "Commit": false
    },
    // "git-graph.fetchAvatars": false,
    "git-graph.initialLoadCommits": 50,
    "git-graph.graphColours": [
        "#0085d9",
        "#d9008f",
        "#00d90a",
        "#d98500",
        "#a300d9",
        "#ff0000",
        "#00d9cc",
        "#e138e8",
        "#85d900",
        "#dc5b23",
        "#6f24d6",
        "#ffcc00"
    ],
    // "git-graph.graphStyle": "rounded",
    "git-graph.loadMoreCommits": 50,
    // "git-graph.tabIconColourTheme": "colour",
    /* markdown */
    "markdown.preview.breaks": true,
    "markdown.preview.fontFamily": "'PT Serif','Hiragino Sans GB','Microsoft YaHei','Times New Roman'",
    "markdown.preview.lineHeight": 1.5,
    "markdown.styles": [],
    "[markdown]": {
        "files.trimTrailingWhitespace": false,
        // "editor.wordWrap": "on",
    },
    "markdownExtended.imageOmitBackground": true,
    "markdownExtended.pdfDisplayHeaderFooter": false,
    "markdownExtended.pdfHeaderTemplate": "<div style='font-size: 9px; margin: 0 auto;'><span class='title'></span></div><div style='font-size: 9px; margin: 0 auto;'><span class='date'></span></div>",
    "markdownExtended.pdfFooterTemplate": "<div style='font-size: 9px; margin: 0 auto;'><span class='pageNumber'></span> / <span class='totalPages'></span></div>",
    "markdownExtended.pdfMarginTop": "0cm",
    "markdownExtended.pdfMarginBottom": "0cm",
    "markdownExtended.pdfMarginLeft": "0cm",
    "markdownExtended.pdfMarginRight": "0cm",
    "markdownExtended.tocLevels": [
        1,
        2,
        3,
        4
    ],
    /* java */
    "[java]": {
        "editor.rulers": [
            80,
            120,
            140
        ],
        "editor.wordWrap": "off"
    },
    /* javascript */
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.rulers": [
            80,
            120,
            140
        ],
        "editor.wordWrap": "off"
    },
    "javascript.format.enable": false,
    "javascript.format.insertSpaceAfterCommaDelimiter": true,
    "javascript.format.insertSpaceAfterConstructor": false,
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
    "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": false,
    "javascript.format.insertSpaceAfterSemicolonInForStatements": true,
    "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": false,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": false,
    /* html */
    "[html]": {
        "editor.rulers": [
            80,
            120,
            140
        ],
        "editor.wordWrap": "off"
    },
    "html.format.enable": false,
    "html.format.endWithNewline": false,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.extraLiners": "head,body,/html",
    "html.format.contentUnformatted": "pre,code,textarea",
    "html.format.unformatted": "wbr,b,i",
    "html.format.maxPreserveNewLines": 1,
    "html.format.preserveNewLines": true,
    // "html.format.wrapAttributes": "auto",
    "html.format.wrapLineLength": 120,
    "htmlhint.enable": true,
    "htmlhint.options": {
        "tagname-lowercase": true,
        "attr-lowercase": true,
        "attr-value-double-quotes": false,
        "doctype-first": false,
        "tag-pair": true,
        "spec-char-escape": true,
        "id-unique": true,
        "src-not-empty": true,
        "attr-no-duplication": true,
        "title-require": true
    },
    /* css */
    "[css]": {
        "editor.rulers": [
            80,
            120,
            140
        ],
        "editor.wordWrap": "off"
    },
    "css.validate": true,
    "css.lint.duplicateProperties": "warning",
    "css.lint.fontFaceProperties": "ignore",
    "css.lint.zeroUnits": "warning",
    "css.remoteStyleSheets": ["https://maxcdn.bootstrapcdn.com/bootstrap/latest/css/bootstrap.min.css"],
    /* beautify */
    "beautify.config": {
        // all
        "eol": "\n", // 默认:"\n"
        "end_with_newline": false, // 默认:false
        "indent_char": " ", // 默认:" "
        "indent_size": 4, // 默认:4
        "indent_with_tabs": false, // 默认:false
        "preserve_newlines": true, // 默认:true
        // js, html
        "brace_style": "collapse,preserve-inline", // 默认:"collapse"
        "max_preserve_newlines": 1, // 默认:10
        "wrap_line_length": 0, // 默认:0
        // js
        "break_chained_methods": false, // 默认:false
        "comma_first": false, // 默认:false
        "e4x": false, // 默认:false
        "indent_level": 0, // 默认:0
        "jslint_happy": false, // 默认:false
        "keep_array_indentation": true, // 默认:false
        "keep_function_indentation": false, // 默认:false
        "operator_position": "after-newline", // 默认:"before-newline"
        "space_after_anon_function": true, // 默认:false
        "space_before_conditional": true, // 默认:true
        "space_in_empty_paren": false, // 默认:false
        "space_in_paren": false, // 默认:false
        "unescape_strings": false, // 默认:false
        "unindent_chained_methods": false, // 默认:false
        // html
        "extra_liners": [
            // 默认
            "head",
            "body",
            "/html"
            // 自定义
        ],
        "indent_body_inner_html": true, // 默认:true
        "indent_handlebars": false, // 默认:false
        "indent_head_inner_html": true, // 默认:true
        "indent_inner_html": false, // 默认:false
        "indent_scripts": "normal", // 默认:"normal"
        "wrap_attributes": "auto", // 默认:"auto"
        "wrap_attributes_indent_size": 4, // 默认:同'indent_size'
        "unformatted": [
            // 默认
            // "a",
            "abbr",
            "area",
            "audio",
            "b",
            "bdi",
            "bdo",
            // "br",
            // "button",
            "canvas",
            "cite",
            "code",
            "data",
            "datalist",
            "del",
            "dfn",
            "em",
            "embed",
            "i",
            // "iframe",
            // "img",
            // "input",
            "ins",
            "kbd",
            "keygen",
            // "label",
            "map",
            "mark",
            "math",
            "meter",
            "noscript",
            "object",
            "output",
            "progress",
            "q",
            "ruby",
            "s",
            "samp",
            // "select",
            "small",
            // "span",
            "strong",
            "sub",
            "sup",
            "svg",
            "template",
            // "textarea",
            "time",
            "u",
            "var",
            "video",
            "wbr",
            "text",
            "acronym",
            "address",
            "big",
            // "dt",
            "ins",
            "strike",
            // "tt"
            // 自定义
        ],
        "content_unformatted": [
            // 默认
            "pre"
            // 自定义
        ],
        "void_elements": [
            // 默认
            "area",
            "base",
            "br",
            "col",
            "embed",
            "hr",
            "img",
            "input",
            "keygen",
            "link",
            "menuitem",
            "meta",
            "param",
            "source",
            "track",
            "wbr",
            "!doctype",
            "?xml",
            "?php",
            "basefont",
            "isindex"
            // 自定义
        ],
        // css
        "newline_between_rules": false, // 默认:false
        "selector_separator_newline": true, // 默认:true
        "space_around_combinator": false, // 默认:false
        // ？？
        // "eval_code": false, // 默认:
    },
    "beautify.ignore": [],
    "beautify.language": {
        "js": {
            "type": [
                "javascript"
            ],
            "filename": [
                ".jshintrc",
                ".jsbeautifyrc"
            ]
        },
        "css": [
            "css",
            "scss"
        ],
        "html": [
            "htm",
            "html"
        ]
    },
    /* emmet */
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "emmet.triggerExpansionOnTab": true,
    /* xml */
    "[xml]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.rulers": [
            80,
            120,
            140
        ],
        "editor.wordWrap": "off"
    },
    /* yaml */
    "[yaml]": {
        "editor.rulers": [
            80,
            120,
            140
        ],
        "editor.tabSize": 2,
        "editor.autoIndent": true
    },
    /* json */
    "json.format.enable": false,
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.rulers": [
            80,
            120,
            140
        ],
        "editor.wordWrap": "off"
    },
    "[jsonc]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.rulers": [
            80,
            120,
            140
        ],
        "editor.wordWrap": "on"
    },
    /* python */
    "[python]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.rulers": [
            80,
            120,
            140
        ],
        "editor.wordWrap": "off"
    },
    /* comments */
    "commentAnchors.tags.list": [
        {
            "tag": "MARK",
            "iconColor": "default",
            "highlightColor": "#A8C023",
            "scope": "file"
        },
        {
            "tag": "ANCHOR",
            "highlightColor": "",
            "scope": "file",
            "enabled": false
        },
        {
            "tag": "TODO",
            "iconColor": "blue",
            "highlightColor": "#3ea8ff",
            "scope": "workspace"
        },
        {
            "tag": "FIXME",
            "iconColor": "red",
            "highlightColor": "#F44336",
            "scope": "workspace"
        },
        {
            "tag": "NOTE",
            "iconColor": "orange",
            "highlightColor": "#FFB300",
            "scope": "file"
        },
        {
            "tag": "REVIEW",
            "highlightColor": "",
            "scope": "file",
            "enabled": false
        },
        {
            "tag": "SECTION",
            "highlightColor": "",
            "scope": "file",
            "enabled": false
        }
    ],
    "commentAnchors.tags.matchCase": false,
    "commentAnchors.tags.separators": [
        " ",
        ":"
    ],
    "commentAnchors.workspace.excludeFiles": "**/{node_modules,.git,.idea,target,out,build,vendor,lib}/**/*",
    "commentAnchors.workspace.lazyLoad": true,
    /* bookmark */
    "bookmarks.navigateThroughAllFiles": true,
    /* togglequotes */
    "togglequotes.chars": [
        "\"",
        "'"
    ],
    "editor.matchBrackets": false,
    /* bracket-pair-colorizer */
    "bracket-pair-colorizer-2.highlightActiveScope": true,
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "bracket-pair-colorizer-2.showHorizontalScopeLine": false
}
